    <!-- Main Menu Tiles -->
    <!-- First tile -->
    <system:String x:Key="Title1_Description">EasySave : A quick and easy backup app</system:String>
    <system:String x:Key="First1_Line_Description">This app allow you to run easily your backup jobs with our intuitive interface !</system:String>
    <system:String x:Key="Second1_Line_Description">You can backup from any folder, to a multitude of platforms :</system:String>
    <system:String x:Key="Third11_Line_Description">- Internal and external hard drive disk</system:String>
    <system:String x:Key="Third12_Line_Description">- Internal network</system:String>
    <system:String x:Key="Third13_Line_Description">- NAS</system:String>
    <system:String x:Key="Third14_Line_Description">- SD card</system:String>
    
    <!-- Second tile -->
    <system:String x:Key="Title2_Description" xml:space="preserve">Secure your data&#xA;with CryptoSoft !</system:String>
    <system:String x:Key="First2_Line_Description" xml:space="preserve">CryptoSoft is a data&#xA;encryption software integra-&#13;ted to EasySave, to permite&#13;you to backup your jobs&#13;safely.</system:String>

    <!-- Third tile -->
    <system:String x:Key="Title3_Description" xml:space="preserve">Create a backup job&#xA;in one click !</system:String>
    <system:String x:Key="First3_Line_Description" xml:space="preserve">But what is a backup job ?&#xA;What we call a backup job is&#13;one or more backup you do&#13;regularly and you want to&#13;automate.</system:String>
    <system:String x:Key="Second3_Line_Description" xml:space="preserve">To create a backup job, please go&#xA;on the« create a backup job »&#13;section,on the left of your screen.</system:String>

    <!-- Fourth tile -->
    <system:String x:Key="Title4_Description" xml:space="preserve">Modify your backup&#13;jobs or delete those&#xA;you no longer need !</system:String>
    <system:String x:Key="First4_Line_Description" xml:space="preserve">EasySave allow you to edit&#xA;your backup jobs parame-&#13;ters, or to delete them.</system:String>
    <system:String x:Key="Second4_Line_Description" xml:space="preserve">To edit or delete a backup job,&#13;please go on the « Edit a backup&#13;job » or « delete a backup job »&#13;section, on the left of your screen.</system:String>

    <!-- Fifth tile -->
    <system:String x:Key="Title5_Description" xml:space="preserve">Run one or more&#13;backup jobs&#xA;simultaneously !</system:String>
    <system:String x:Key="First5_Line_Description" xml:space="preserve">Run one or more backup&#xA;jobs simultaneously by&#13;selectionning those you&#13;want to run.</system:String>
    <system:String x:Key="Second5_Line_Description" xml:space="preserve">To run one or more backup jobs,&#13; please go on the « run one or&#13;more backup jobs» section, on&#13;the left of your screen.</system:String>


